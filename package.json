  },
  "devDependencies": {
    "eslint-config-custom": "workspace:0.0.0",
    "prettier": "^2.8.0",
    "turbo": "^1.6.3"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.5.2"
}